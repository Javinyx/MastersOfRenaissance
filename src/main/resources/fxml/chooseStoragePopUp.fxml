    <ImageView fx:id="resource2ImgPU" fitHeight="30.0" layoutX="109.0" layoutY="167.0" preserveRatio="true"
               styleClass="resImg"/>
    <ImageView fx:id="resource3ImgPU" fitHeight="30.0" layoutX="109.0" layoutY="217.0" preserveRatio="true"
