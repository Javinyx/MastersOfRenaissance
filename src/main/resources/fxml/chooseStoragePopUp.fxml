    <Region fx:id="shelf32PU" layoutX="260.0" layoutY="234.0" prefHeight="50.0" prefWidth="40.0"/>
    <Region fx:id="shelf33PU" layoutX="300.0" layoutY="234.0" prefHeight="50.0" prefWidth="50.0"/>

    <!-- Resources inside warehouse -->
    <ImageView fx:id="shelf1ImgPU" fitHeight="30.0" preserveRatio="true" styleClass="resImg"/>
    <ImageView fx:id="shelf21ImgPU" fitHeight="30.0" preserveRatio="true" styleClass="resImg"/>
    <ImageView fx:id="shelf22ImgPU" fitHeight="30.0" preserveRatio="true" styleClass="resImg"/>
